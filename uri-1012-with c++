#include <bits/stdc++.h>

using namespace std;

int main()
{
    double A,B,C;
    double pi=3.14159;
    double TRIANGULO,CIRCULO,TRAPEZIO,QUADRADO,RETANGULO;
    
    cin>>A>>B>>C;
    TRIANGULO=(A*C)/2;
    CIRCULO=(pi*pow(C,2));
    TRAPEZIO=(A+B)*C/2;
    QUADRADO=pow(B,2);
    RETANGULO=A*B;
    cout<<fixed;
    cout<<setprecision(3)<<"TRIANGULO: "<<TRIANGULO<<"\n";
    cout<<setprecision(3)<<"CIRCULO: "<<CIRCULO<<"\n";
    cout<<setprecision(3)<<"TRAPEZIO: "<<TRAPEZIO<<"\n";
    cout<<setprecision(3)<<"QUADRADO: "<<QUADRADO<<"\n";
    cout<<setprecision(3)<<"RETANGULO: "<<RETANGULO<<"\n";

    return 0;
}
